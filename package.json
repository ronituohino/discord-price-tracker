{
  "name": "price-tracker",
  "version": "1.0.0",
  "description": "Price tracking Discord bot",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "docker-compose up",
    "dev": "docker-compose -f docker-compose.dev.yml up",
    "rebuild": "docker-compose -f docker-compose.dev.yml build",
    "docker:dev": "nodemon --esm src/index.ts",
    "docker:build": "tsc",
    "docker:start": "node index.js"
  },
  "author": "Roni Tuohino",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.3",
    "node-cron": "^3.0.2",
    "pg": "^8.10.0",
    "puppeteer": "^20.2.0"
  },
  "devDependencies": {
    "@types/node-cron": "^3.0.7",
    "@types/pg": "^8.6.6",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
